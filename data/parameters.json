{
    "solid_earth": {
        "model": {
            "use_long_term_anelasticity": true,
            "use_short_term_anelasticity": true,
            "use_bounded_attenuation_functions": true,
            "dynamic_term": true,
            "real_crust": false,
            "radius_unit": null,
            "below_icb_layers": null,
            "below_cmb_layers": null
        },
        "frequency_discretization": {
            "period_min_year": 1e-1,
