            "n_frequency_0": 30,
            "max_tol": 0.05,
            "decimals": 2
        },
        "degree_discretization": {
            "steps": [
                1,
                2,
                5
            ],
            "thresholds": [
                1,
                20,
                40,
                100
            ]
        },
        "numerical_parameters": {
            "spline_number": 10,
            "spline_degree": 1,
            "n_max_for_sub_cmb_integration": 35,
            "minimal_radius": 1e3,
            "method": "DOP853",
            "atol": 1e-14,
            "rtol": 1e-11,
            "t_eval": null
        },
        "options": {
            "compute_Green": true,
            "load_numerical_model": false,
            "model_id": null,
            "save": true,
            "overwrite_model": false
        }
    },
    "load": {
        "save_options": {
            "json_harmonics": {
                "all": true,
                "step_1": false,
                "step_2": false,
                "step_3": false,
                "step_4": false,
                "step_5": false,
                "inversion_components": false
            },
            "binary_harmonics": {
                "all": true,
                "step_1": false,
                "step_2": false,
                "step_3": false,
                "step_4": false,
                "step_5": false,
                "inversion_components": false
            }
        },
        "model": {
            "numerical_parameters": {
                "leakage_correction_iterations": 1,
                "renormalize_recent_trend": true,
                "initial_past_trend_factor": 1.22,
                "anti_Gibbs_effect_factor": 0,
                "spline_time_years": 50,
                "initial_plateau_time_years": 2000,
                "signal_threshold": 12.0,
                "signal_threshold_past": 6.0,
                "mean_signal_threshold": null,
                "mean_signal_threshold_past": null,
                "ddk_filter_level": 5,
                "ocean_mask": "IMERG_land_sea_mask.nc",
